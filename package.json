{
  "name": "finalware",
  "version": "1.0.0",
  "description": "tool to flash starlight-12",
  "main": "src/main.js",
  "scripts": {
    "start": "sass src/:app/ && electron .",
    "win-fix": "del /s /q node_modules && yarn install && \"./node_modules/.bin/electron-rebuild.cmd\"",
    "linux-fix": "./node_modules/.bin/electron-rebuild",
    "build:linux": "electron-builder --linux",
    "build:win": "electron-builder --win",
    "postinstall": "electron-builder install-app-deps"
  },
  "author": "kuromis-2<ilovepoggers3@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "electron": "^18.2.0",
    "electron-builder": "^23.0.3",
    "electron-rebuild": "^3.2.7"
  },
  "dependencies": {
    "@popperjs/core": "^2.10.2",
    "cache": "^2.3.1",
    "clear": "^0.1.0",
    "node-gyp": "^9.0.0",
    "npm": "^8.8.0",
    "pc-nrf-dfu-js": "^0.2.12",
    "request": "^2.88.2",
    "sass": "^1.51.0",
    "serialport": "^8.0.7",
    "yarn": "^1.22.18"
  },
  "build": {
    "appId": "com.kuromis-2.finalware",
    "linux": {
      "target": [
        "deb",
        "rpm",
        "AppImage",
        "snap"
      ],
      "icon": "./app/finalmouse-logo.png",
      "category": "Utility"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "./app/finalmouse-logo.png",
      "category": "Utility"
    }
  }
}
